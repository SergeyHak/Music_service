{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "prettier",
        "plugin:react/recommended",
        "plugin:react/jsx-runtime"
    ],
    "paeserOptions":{
        "ecmaFeatures":{
            "jsx":true
        },
        "ecmaVersion":"latest",
        "sourceType":"module"
    },
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
    "react/jsx-filename-extension":[2,{"extension":[".js",".jsx"]}],
    "react/prop-types":0,
    "react/destructuring-assigment":[0]
    }
}
